<% html_title l(:label_settings), l(:issues_catalog) %>
<%= stylesheet_link_tag('issues_catalog', plugin: 'issues_catalog') %>
<%= javascript_include_tag('settings', plugin: 'issues_catalog') %>
<%= javascript_tag("const IssuesCatalogSettingParam = #{create_issues_catalog_settings_context(@project).to_json};") %>

<%
catalog_tabs = [
  {
    name: 'manage_tag_categories',
    partial: 'issues_catalog_settings/manage_tag_categories',
    url: {tab: 'issues_catalog', sub_tab: 'manage_tag_categories'},
    label: :label_catalog_tag_categories
  },
  {
    name: 'manage_tag_groups',
    partial: 'issues_catalog_settings/manage_tag_groups',
    url: {tab: 'issues_catalog', sub_tab: 'manage_tag_groups'},
    label: :label_catalog_tag_groups
  },
  {
    name: 'manage_tags',
    partial: 'issues_catalog_settings/manage_tags',
    url: {tab: 'issues_catalog', sub_tab: 'manage_tags'},
    label: :label_tags
  },
  {
    name: 'manage_synonyms',
    partial: 'issues_catalog_settings/manage_synonyms',
    url: {tab: 'issues_catalog', sub_tab: 'manage_synonyms'},
    label: :label_synonyms
  },
]
%>
<%= render_tabs(catalog_tabs) %>
